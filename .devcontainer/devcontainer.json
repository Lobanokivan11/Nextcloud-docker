{
  "name": "Nextcloud",
  "service": "app",
  "forwardPorts": [80],
  "portsAttributes": {
    "80": {
      "label": "Nextcloud Server",
      "onAutoForward": "notify"
    }
  }, 
  "postCreateCommand": "gh codespace ports visibility 80:public -c $CODESPACE_NAME", 
  "otherPortsAttributes": {
    "onAutoForward": "notify"
  }, 
  "dockerComposeFile": "codespaces.yml", 
  "customizations": {
    "vscode": {
      "settings": {
        "codespaces.shutdownWhenInactive": false
      }
    }
  }
}
